{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false,
        "strictNullChecks": true,
        "noEmitOnError": false,
        "noUnusedParameters": true,
        "outDir": "dist/",
        "experimentalDecorators": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        ".DS_Store"
    ]
}
